{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-covidproject"
		},
		"ls_ablob_covidprojectstorage123_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidprojectstorage123'"
		},
		"ls_population_covidprojectdatalake123_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_population_covidprojectdatalake123'"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"ls_population_covidprojectdatalake123_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidprojectdatalake123.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-covidproject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidprojectstorage123_connectionString": {
						"value": "[parameters('ls_ablob_covidprojectstorage123_connectionString')]"
					},
					"ls_population_covidprojectdatalake123_accountKey": {
						"value": "[parameters('ls_population_covidprojectdatalake123_accountKey')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					},
					"ls_population_covidprojectdatalake123_properties_typeProperties_url": {
						"value": "[parameters('ls_population_covidprojectdatalake123_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}