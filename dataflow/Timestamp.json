{
	"name": "Timestamp",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "logdata",
						"type": "DatasetReference"
					},
					"name": "Timeflow"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "changetime"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as string,",
				"          Correlationid as string,",
				"          Operationname as string,",
				"          Status as string,",
				"          Eventcategory as string,",
				"          Level as string,",
				"          Time as string,",
				"          Subscription as string,",
				"          Eventinitiatedby as string,",
				"          Resourcetype as string,",
				"          Resourcegroup as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Timeflow",
				"Timeflow derive(Time = toTimestamp(concat(substring(Time,0,10),' ',substring(Time,12,8)),'yyyy-MM-dd HH:mm:ss')) ~> changetime",
				"changetime sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['logtime.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1"
			]
		}
	}
}